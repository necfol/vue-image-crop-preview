    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-stage-2": "^6.24.1",
        "babel-preset-vue": "^2.0.2",
        "cross-env": "^5.1.6",
        "eslint": "^4.19.1",
        "eslint-config-necfol": "^1.0.4",
        "eslint-config-vue": "^2.0.2",
        "husky": "^0.14.3",
        "lint-staged": "^7.1.2",
        "prettier": "^1.12.1",
        "webpack": "^4.9.1",
        "webpack-cli": "^2.1.4"
    },
    "dependencies": {
        "vue": "^2.5.16"
    }
}
